# "input" function lets you ask a user for some text input or number
name = input("What is your name:")

# testing input
print("I'm Python. Nice to meet you,",name)

# library imports
import numpy as np
import pandas as pd
import holoviews as hv
hv.extension('bokeh', logo=False)

# create sample data
data = np.random.normal(size=[50, 2])
df = pd.DataFrame(data, columns=['col1', 'col2'])

# create holoviews graph
hv_plot = hv.Points(df)
hv_plot.opts(tools=['hover'])
hv_plot
